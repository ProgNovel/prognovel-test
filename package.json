{
	"name": "@prognovel/frontend",
	"description": "Next-Generation WebNovel platform.",
	"version": "0.3.2",
	"scripts": {
		"populate": "node plugins/build/populate/assets && node plugins/build/populate/copy-assets",
		"dev": "sapper dev --no-hot --ext \".svelte .svx\"",
		"build": "npm run populate && npm run export && node plugins/build/finish",
		"build:functions": "netlify-lambda build ./src/.netlify-functions --config ./webpack.functions.js --babelrc false",
		"build:serve": "npm run export && serve __sapper__/export",
		"build:publish": "npm run populate && npm run build",
		"export": "sapper export --ext \".svelte .svx\" --timeout 60000",
		"test": "cypress run",
		"test:cy": "cypress open",
		"format": "prettier --write src/**/*.{ts,js,svelte} --plugin-search-dir=.",
		"ilp:stream": "node interledger/ilp-stream-server"
	},
	"dependencies": {
		"@fortawesome/free-brands-svg-icons": "^5.15.3",
		"@fortawesome/free-regular-svg-icons": "^5.15.3",
		"@fortawesome/free-solid-svg-icons": "^5.15.3",
		"@typescript-eslint/eslint-plugin": "^4.0.0",
		"chalk": "^4.1.0",
		"compression": "^1.7.1",
		"date-fns": "^2.16.1",
		"dotenv": "^8.2.0",
		"eslint": "^7.16.0",
		"eslint-config-airbnb-base": "^14.2.1",
		"extract-zip": "^2.0.1",
		"fa-svelte": "^3.1.0",
		"front-matter": "^4.0.2",
		"globby": "^11.0.3",
		"hammerjs": "^2.0.8",
		"idb-keyval": "^5.1.3",
		"js-crypto-aes": "^1.0.2",
		"jsdom": "^16.4.0",
		"marked": "^1.2.7",
		"md5": "^2.3.0",
		"ncp": "^2.0.0",
		"node-fetch": "^2.6.1",
		"polka": "^0.5.0",
		"sharp": "^0.29.2",
		"sirv": "^1.0.10",
		"web-vitals": "^1.0.1",
		"webfunding": "latest"
	},
	"devDependencies": {
		"@babel/cli": "^7.12.10",
		"@babel/core": "^7.12.10",
		"@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
		"@babel/plugin-proposal-optional-chaining": "^7.12.7",
		"@babel/plugin-proposal-pipeline-operator": "^7.12.13",
