		"@types/aws-lambda": "^8.10.67",
		"@typescript-eslint/parser": "^4.0.0",
		"assets-webpack-plugin": "^6.1.2",
		"autoprefixer": "^10.1.0",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.2.2",
		"carbon-components-svelte": "^0.26.0",
		"cypress": "^6.2.0",
		"disqus-svelte": "^1.0.9",
		"duplicate-package-checker-webpack-plugin": "^3.0.0",
		"eslint-config-prettier": "^8.3.0",
